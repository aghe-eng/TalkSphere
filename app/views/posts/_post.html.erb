<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= post.content %>
  </p>

  <p>
    <strong>User:</strong>
    <%= post.user_id_id %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= post.category_id_id %>
  </p>

</div>
